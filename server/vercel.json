{
  "version": 2,
  "functions": {
    "index.js": {
      "memory": 1024,
      "includeFiles": "db.json"
    }
  },

  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node",
      "config": { "includeFiles": ["./db.json"] }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ]
}
